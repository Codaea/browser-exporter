{
    "name": "Chrome Exporter",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "public/popup.html"
    },
    "background": {
        "service_worker": "dist/background.js"
    },
    "host_permissions": [
        "https://chrome.codaealab.com/metrics/job/chrome-exporter"
    ],
    "permissions": [
        "alarms",
        "tabs",
        "windows",
        "bookmarks",
        "system.memory",
        "system.cpu",
        "webRequest",
        "readingList",
        "storage"
    ]
}